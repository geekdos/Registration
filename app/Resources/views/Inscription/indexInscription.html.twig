{% extends '::layout.html.twig' %}
{% block body %}
    <div class="sortable">
        <div class="row">
            {% if inscriptionDeug %}
            <div class="col-md-6">
                <div class="panel panel-red">
                    <div class="panel-heading">
                        <h3 class="panel-title">DEUG || تسجيل</h3>
                        <div class="panel-control">
                            <a href="javascript:void(0);" data-toggle="tooltip" data-placement="top" title="Expand/Collapse" class="panel-collapse"><i class="icon-arrow-down"></i></a>
                            <a href="javascript:void(0);" data-toggle="tooltip" data-placement="top" title="Reload" class="panel-reload"><i class="icon-reload"></i></a>
                            <a href="javascript:void(0);" data-toggle="tooltip" data-placement="top" title="Remove" class="panel-remove"><i class="icon-close"></i></a>
                        </div>
                    </div>
                    <div class="panel-body">
                        {{ inscriptionDeug.value | raw }}
                    </div>
                    <div class="panel-footer">
                        <p>
                            <a href="{{ path('etudiantdeug_new') }}" class="btn btn-danger btn-addon m-b-sm pull-right">تسجيل</a>
                            <a href="{{ path('etudiantdeug_new') }}" class="btn btn-danger btn-addon m-b-sm pull-left">Inscription</a>
                        </p>
                    </div>
                </div>
                {% endif %}
                {% if inscriptionMaster %}
                <div class="panel panel-blue">
                    <div class="panel-heading">
                        <h3 class="panel-title">Master || الماستر</h3>
                        <div class="panel-control">
                            <a href="javascript:void(0);" data-toggle="tooltip" data-placement="top" title="Expand/Collapse" class="panel-collapse"><i class="icon-arrow-down"></i></a>
                            <a href="javascript:void(0);" data-toggle="tooltip" data-placement="top" title="Reload" class="panel-reload"><i class="icon-reload"></i></a>
                            <a href="javascript:void(0);" data-toggle="tooltip" data-placement="top" title="Remove" class="panel-remove"><i class="icon-close"></i></a>
                        </div>
                    </div>
                    <div class="panel-body">
                        {{ inscriptionMaster.value | raw }}
                    </div>
                    <div class="panel-footer">
                        <p>
                            <a href="{{ path('etudiantmaster_new') }}" class="btn btn-info btn-addon m-b-sm pull-right">تسجيل</a>
                            <a href="{{ path('etudiantmaster_new') }}" class="btn btn-info btn-addon m-b-sm pull-left">Inscription</a>
                        </p>
                    </div>
                </div>
                {% endif %}
            </div>
            <div class="col-md-6">
                {% if inscriptionLicence %}
                <div class="panel panel-purple">
                    <div class="panel-heading">
                        <h3 class="panel-title">Licence || الإجازة</h3>
                        <div class="panel-control">
                            <a href="javascript:void(0);" data-toggle="tooltip" data-placement="top" title="Expand/Collapse" class="panel-collapse"><i class="icon-arrow-down"></i></a>
                            <a href="javascript:void(0);" data-toggle="tooltip" data-placement="top" title="Reload" class="panel-reload"><i class="icon-reload"></i></a>
                            <a href="javascript:void(0);" data-toggle="tooltip" data-placement="top" title="Remove" class="panel-remove"><i class="icon-close"></i></a>
                        </div>
                    </div>
                    <div class="panel-body">
                        {{ inscriptionLicence.value | raw }}
                    </div>
                    <div class="panel-footer">
                        <p>
                            <a href="{{ path('etudiantlicence_new') }}" class="btn btn-primary btn-addon m-b-sm pull-right">تسجيل</a>
                            <a href="{{ path('etudiantlicence_new') }}" class="btn btn-primary btn-addon m-b-sm pull-left">Inscription</a>
                        </p>
                    </div>
                </div>
                {% endif %}
                {% if inscriptionDoctorat %}
                <div class="panel panel-green">
                    <div class="panel-heading">
                        <h3 class="panel-title">Doctrorat || الدكتورة</h3>
                        <div class="panel-control">
                            <a href="javascript:void(0);" data-toggle="tooltip" data-placement="top" title="Expand/Collapse" class="panel-collapse"><i class="icon-arrow-down"></i></a>
                            <a href="javascript:void(0);" data-toggle="tooltip" data-placement="top" title="Reload" class="panel-reload"><i class="icon-reload"></i></a>
