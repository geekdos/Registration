                        </div>
                    </div>
                    <div class="panel-body">
                        {{ inscriptionDoctorat.value | raw }}
                    </div>
                    <div class="panel-footer">
                        <p>
                            <a href="{{ path('etudiantdoctorat_new') }}" class="btn btn-success btn-addon m-b-sm pull-right">تسجيل</a>
                            <a href="{{ path('etudiantdoctorat_new') }}" class="btn btn-success btn-addon m-b-sm pull-left">Inscription</a>
                        </p>
                    </div>
                </div>
                {% endif %}
            </div>
        </div><!-- Row -->
    </div><!-- Sortable -->
{% endblock %}