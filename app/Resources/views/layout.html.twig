<!DOCTYPE html>
<html>
<head>
    {# Title #}
    <title>{{ "now"|date("Y") }}-Re-Inscription{% block title %}{% endblock %}</title>
    {# META SECTION #}
    <meta charset="UTF-8"/>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <meta name="viewport" content="width=device-width, initial-scale=1"/>
    <meta name="description" content="Platforme d'incription pour les étudiants de la faculté des lettres saiss de Fés"/>
    <meta name="keywords" content="inscription, re-inscription, fl saiss fes"/>
    <meta name="author" content="khachiai oussama"/>
    {# END META SECTION #}
    {% block stylesheets %}
        {# Styles SECTION #}
        <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600' rel='stylesheet' type='text/css'>
        <link href="{{ asset('bundles/reregistration/assets/plugins/pace-master/themes/blue/pace-theme-flash.css') }}"
              rel="stylesheet"/>
        <link href="{{ asset('bundles/reregistration/assets/plugins/uniform/css/uniform.default.min.css') }}"
              rel="stylesheet"/>
        <link href="{{ asset('bundles/reregistration/assets/plugins/bootstrap/css/bootstrap.min.css') }}"
              rel="stylesheet" type="text/css"/>
        <link href="{{ asset('bundles/reregistration/assets/plugins/fontawesome/css/font-awesome.css') }}"
              rel="stylesheet" type="text/css"/>
        <link href="{{ asset('bundles/reregistration/assets/plugins/line-icons/simple-line-icons.css') }}"
              rel="stylesheet" type="text/css"/>
        <link href="{{ asset('bundles/reregistration/assets/plugins/offcanvasmenueffects/css/menu_cornerbox.css') }}"
              rel="stylesheet" type="text/css"/>
        <link href="{{ asset('bundles/reregistration/assets/plugins/waves/waves.min.css') }}" rel="stylesheet"
              type="text/css"/>
        <link href="{{ asset('bundles/reregistration/assets/plugins/switchery/switchery.min.css') }}" rel="stylesheet"
              type="text/css"/>
        <link href="{{ asset('bundles/reregistration/assets/plugins/3d-bold-navigation/css/style.css') }}"
              rel="stylesheet" type="text/css"/>
        <link href="{{ asset('bundles/reregistration/assets/plugins/slidepushmenus/css/component.css') }}"
              rel="stylesheet" type="text/css"/>
        {# END Styles SECTION #}
        {# Theme Styles SECTION #}
        <link href="{{ asset('bundles/reregistration/assets/css/modern.min.css') }}" rel="stylesheet" type="text/css"/>
        <link href="{{ asset('bundles/reregistration/assets/css/themes/white.css') }}" class="theme-color"
              rel="stylesheet" type="text/css"/>
        <link href="{{ asset('bundles/reregistration/assets/css/custom.css') }}" rel="stylesheet" type="text/css"/>

        <script src="{{ asset('bundles/reregistration/assets/plugins/3d-bold-navigation/js/modernizr.js') }}"></script>
        <script src="{{ asset('bundles/reregistration/assets/plugins/offcanvasmenueffects/js/snap.svg-min.js') }}"></script>
        {# END Theme Styles SECTION #}

        <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
    {% endblock %}

</head>
<body class="page-header-fixed page-horizontal-bar">
<div class="overlay"></div>
{% include ':include:header.html.twig' %}
<main class="page-content content-wrap">
    {% include ':include:menu.html.twig' %}
    <div class="page-inner">
        <div class="page-title">
            <h3><span>Service se scolarité</span><span class="pull-right">مصلحة شؤن الطلبة</span></h3>
            <div class="page-breadcrumb">
                <ol class="breadcrumb">
                    <li><a href="{{ path('reregistration_homepage') }}">FL-SAISS</a></li>
                    <li class="active"><span>Service se scolarité</span> | <span class="pull-right"> مصلحة شؤن الطلبة </span></li>
                </ol>
            </div>
        </div>
        <div id="main-wrapper">
            <div class="row">
                {% block body %}{% endblock body %}
            </div><!-- Row -->
        </div><!-- Main Wrapper -->
        {% include ':include:footer.html.twig' %}
    </div><!-- Page Inner -->
</main><!-- Page Content -->

{% block javascripts %}
    {# Javascripts #}
    <script src="{{ asset('bundles/reregistration/assets/plugins/jquery/jquery-2.1.3.min.js') }}"></script>
    <script src="{{ asset('bundles/reregistration/assets/plugins/jquery-ui/jquery-ui.min.js') }}"></script>
    <script src="{{ asset('bundles/reregistration/assets/plugins/pace-master/pace.min.js') }}"></script>
    <script src="{{ asset('bundles/reregistration/assets/plugins/jquery-blockui/jquery.blockui.js') }}"></script>
    <script src="{{ asset('bundles/reregistration/assets/plugins/bootstrap/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('bundles/reregistration/assets/plugins/jquery-slimscroll/jquery.slimscroll.min.js') }}"></script>
    <script src="{{ asset('bundles/reregistration/assets/plugins/switchery/switchery.min.js') }}"></script>
    <script src="{{ asset('bundles/reregistration/assets/plugins/uniform/jquery.uniform.min.js') }}"></script>
    <script src="{{ asset('bundles/reregistration/assets/plugins/offcanvasmenueffects/js/classie.js') }}"></script>
    <script src="{{ asset('bundles/reregistration/assets/plugins/offcanvasmenueffects/js/main.js') }}"></script>
    <script src="{{ asset('bundles/reregistration/assets/plugins/waves/waves.min.js') }}"></script>
    <script src="{{ asset('bundles/reregistration/assets/plugins/3d-bold-navigation/js/main.js') }}"></script>
    <script src="{{ asset('bundles/reregistration/assets/js/modern.min.js') }}"></script>
    <script src="{{ asset('bundles/reregistration/assets/js/pages/dashboard.js') }}"></script>
    {# Javascripts #}
{% endblock javascripts %}
</body>
</html>
